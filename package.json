{
  "name": "document-distribution-center",
  "version": "0.0.1",
  "description": "Secure document distribution center.",
  "main": "./dist/src/index.js",
  "bin": "./dist/src/cli.js",
  "scripts": {
    "build": "tsc && tsc --declaration && cp -r ./lib dist/ &&copyfiles ./**/*.json dist/ src/**/*.ejs src/**/*.css dist/ dist/ src/**/*.png dist/ dist/ src/**/*.jpg dist/",
    "rebuild": "rm -rf ./dist && npm run build",
    "start": "node ./dist/src/cli.js start",
    "views": "copyfiles src/**/*.ejs src/**/*.css dist/ src/**/*.png dist/ dist/ src/**/*.jpg dist/",
    "view-start": "npm run views && npm run start"
  },
  "author": "“Noah <“noah.bouma@gmail.com”>",
  "license": "ISC",
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/commander": "^2.12.2",
    "@types/express": "^4.16.1",
    "@types/express-session": "^1.15.12",
    "@types/jsonschema": "^1.1.1",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^11.13.4",
    "authid-core-ts": "git+https://github.com/OnePair/authid-core-ts.git#a8d9a98683a4a4c52038bf57dcf1456f3c58d625",
    "authid-eth-driver": "git+https://github.com/OnePair/authid-eth-driver-ts.git#3a0abc40a31046b19156c3f06ced9d73726ab807",
    "body-parser": "^1.18.3",
    "commander": "^2.19.0",
    "copyfiles": "^2.1.0",
    "crypto": "^1.0.1",
    "ethers": "^4.0.27",
    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "ipfs-http-client": "^30.1.3",
    "jsonschema": "^1.2.4",
    "jsonwebtoken": "^8.5.1",
    "level": "^5.0.1",
    "mkdirp": "^0.5.1",
    "tsc": "^1.20150623.0",
    "typescript": "^3.4.4"
  }
}
